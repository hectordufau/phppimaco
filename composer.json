{
    "name": "hectordufau/phppimaco",
    "description": "Lib para geração de Etiquetas PIMACO versao FPDF",
    "keywords": ["PIMACO", "etiquetas","tags","hang tags", "fpdf"],
    "license": "MIT",
    "authors": [
        {
            "name": "Hector Dufau",
            "email": "contato@hectordufau.com.br"
        }
    ],
    "autoload": {
        "psr-4": {
            "HLMI\\PhpPimaco\\": "src/"
        }
    },
    "require": {
        "php": ">=7.0",
        "ext-json": "*",
        "endroid/qr-code": "^2.5",
        "picqer/php-barcode-generator": "^0.4",
        "fpdf/fpdf": "^1.83"
    },
    "autoload-dev": {
        "psr-4": {
            "HLMI\\PhpPimacoTest\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "squizlabs/php_codesniffer": "^3.2"
    }
}
